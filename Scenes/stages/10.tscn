script/source = "extends Node2D

func _ready():
	$area_2d/collision_polygon_2d.polygon=self.polygon
	set_process(true)

func _process(delta):
	self.modulate.r=1
	for body in $area_2d.get_overlapping_bodies():
		if body.is_in_group('Player'):
			get_tree().reload_current_scene() #@Make a better gameover
			self.modulate.r=30"

